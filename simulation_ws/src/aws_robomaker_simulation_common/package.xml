<?xml version='1.0' encoding='utf-8'?>
<package format="3">
  <name>aws_robomaker_simulation_common</name>
  <version>1.0.0</version>
  <description>
    AWS RoboMaker simulation package for commonly used nodes and helpers.
  </description>
  <author email="ros-contributions@amazon.com">AWS RoboMaker</author>
  <maintainer email="ros-contributions@amazon.com">AWS RoboMaker</maintainer>
  <license>Apache 2.0</license>
  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake</buildtool_depend>
  <exec_depend>gazebo_ros</exec_depend>
  <exec_depend>gazebo</exec_depend>
  <exec_depend>gazebo_plugins</exec_depend>
  <exec_depend>moveit_ros</exec_depend>
  <exec_depend>dwa_local_planner</exec_depend>
  <export>
    <gazebo_ros gazebo_media_path="${prefix}" gazebo_model_path="${prefix}/models" plugin_path="${prefix}/lib" />
  </export>
  <export>
    <build_type>ament_cmake</build_type>
  </export>
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
